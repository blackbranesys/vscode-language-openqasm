{
    "name": "vscode-language-openqasm",
    "displayName": "OpenQASM",
    "description": "OpenQASM language support for VS Code without the garbage.",
    "publisher": "blackbrane",
    "version": "0.0.2",
    "preview": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/blackbranesys/vscode-language-openqasm"
    },
    "icon": "openqasm-icon.png",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qasm",
            "aliases": ["OpenQASM", "qasm"],
            "extensions": [".qasm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "qasm",
            "scopeName": "source.qasm",
            "path": "./syntaxes/qasm.tmLanguage.json"
        }]
    }
}
